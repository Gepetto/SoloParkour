  <!-- END UPPER LEG LINK -->
  <!-- KFE: Joint between the upper leg and the lower leg -->
  <joint name="HR_KFE" type="revolute">
    <parent link="HR_UPPER_LEG" />
    <child link="HR_LOWER_LEG" />
    <limit effort="3" lower="-10" upper="10" velocity="20" />
    <!-- joints rotates around the y-axis -->
    <axis xyz="0 1 0" />
    <origin rpy="0 0 0" xyz="0 -0.04 -0.35" />
    <!-- pybullet simulation parameters -->
    <dynamics damping="0.0" friction="0.0" />
  </joint>
  <link name="HR_LOWER_LEG">
    <!-- Right lower leg inertia -->
    <inertial>
      <mass value="0.5" />
      <origin rpy="0 0 0" xyz="0.00000000 -0.020971332 -0.189104652" />
      <inertia ixx="0.00008508" ixy="0.00000000" ixz="0.00000000" iyy="0.00008580" iyz="0.00000200"
        izz="0.00000139" />
    </inertial>
    <!-- LOWER LEG LINK VISUAL -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.175" />
      <geometry>
        <cylinder length="0.35" radius="0.03" />
      </geometry>
      <material name="grey">
        <color rgba="0.8 0.8 0.8 1.0" />
      </material>
    </visual>
    <!-- LOWER LEG LINK COLLISION -->
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.35" />
      <!-- <geometry>
        <mesh filename="meshes/stl/without_foot/solo_lower_leg_v2_right_side.stl" />
      </geometry> -->
      <geometry>
        <sphere radius="0.03" />
      </geometry>
      <material name="grey">
        <color rgba="0.8 0.8 0.8 1.0" />
      </material>
    </collision>
  </link>
  <!-- END LOWER LEG LINK -->
  <!-- Joint between the lower leg and the ankle,
         this is only a dummy joint such that we
         can assign coordinate frames to foot-->
  <joint name="HR_ANKLE" type="fixed" dont_collapse="true">
    <parent link="HR_LOWER_LEG" />
    <child link="HR_FOOT" />
    <origin rpy="0 0 0" xyz="0 -0.0243 -0.35" />
    <!-- Limits -->
    <limit effort="3" lower="-10" upper="10" velocity="20" />
    <!-- pybullet simulation parameters -->
    <dynamics damping="0.0" friction="0.0" />
  </joint>
  <link name="HR_FOOT">
    <!-- FOOT INERTIAL -->
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.00035767" />
      <mass value="0.15" />
      <inertia ixx="0.00000057" ixy="0.0" ixz="0.0" iyy="0.00000084" iyz="0.0" izz="0.00000053" />
    </inertial>
  </link>
  <!-- END FOOT LINK -->
</robot>